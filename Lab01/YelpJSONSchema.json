                {
                  "type": "object",
                  "properties": {
                    "alias": {
                      "type": "string"
                    },
                    "title": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "alias",
                    "title"
                  ]
                }
              ]
            },
            "review_count": {
              "type": "integer"
            },
