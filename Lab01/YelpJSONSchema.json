                "country": {
                  "type": "string"
                },
                "address2": {
                  "type": "string"
                },
                "address3": {
                  "type": "string"
                },
                "state": {
                  "type": "string"
                },
                "address1": {
                  "type": "string"
                },
                "zip_code": {
                  "type": "string"
                }
              },
              "required": [
                "city",
                "country",
                "address2",
                "address3",
                "state",
                "address1",
                "zip_code"
              ]
            },
            "distance": {
              "type": "number"
